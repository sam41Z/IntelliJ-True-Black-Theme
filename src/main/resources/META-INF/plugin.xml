<idea-plugin>
    <id>ch.sam41z.true-black-theme</id>
    <name>True Dark</name>
    <vendor email="ahmed3elshaer@gmail.com" url="https:/github.com/Ahmed3Elshaer">Ahmed Elshaer</vendor>
    <depends>com.intellij.modules.platform</depends>
    <idea-version since-build="223"/>
    <extensions defaultExtensionNs="com.intellij">
        <applicationService serviceImplementation="ch.sam41z.trueblack.settings.PluginMetaSettings"/>
        <postStartupActivity implementation="ch.sam41z.trueblack.activities.StartupActivity"/>
        <notificationGroup displayType="STICKY_BALLOON" id="True Dark," isLogByDefault="true"/>
        <themeProvider id="ae7a917c-04b2-4468-bab7-6ddd4c43f879" path="/TrueBlack.theme.json"/>
        <themeProvider id="ae7a917c-04b2-4468-bdd8-6ddd4c43f879" path="/TrueDark.theme.json"/>
        <themeProvider id="ae7a917c-04b2-4468-bdd9-6ddd4c43f879" path="/TrueLight.theme.json"/>
    </extensions>
</idea-plugin>
